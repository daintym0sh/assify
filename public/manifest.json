{
  "manifest_version": 3,
  "name": "Assify",
  "version": "0.1.0",
  "description": "My Chrome Ass Extension",
  "icons": {
    "16": "icons/icon_36.png"
  },
  "action": {
    "default_title": "Assify",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "js": [
        "contentScript.js"
      ]
    }
  ]
}