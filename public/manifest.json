{
  "manifest_version": 3,
  "name": "Assify",
  "version": "0.0.1",
  "description": "My Chrome Ass Extension. A fun way to spice up adjectives in a web page using the word ass.",
  "icons": {
    "16": "icons/icon_36.png"
  },
  "action": {
    "default_title": "Assify",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "js": [
        "contentScript.js"
      ]
    }
  ]
}